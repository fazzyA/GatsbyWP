{
	"name": "site",
	"version": "2.0.9",
	"description": "Site to test the Gatsby WordPress Theme Phoenix",
	"main": "gatsby-config.js",
	"scripts": {
		"build": "gatsby build",
		"dev": "gatsby develop",
	  	"dev:clean": "gatsby clean && gatsby develop",
		"format": "prettier --write \"**/*.{js,jsx,json,md}\"",
		"start": "npm run dev",
		"serve": "gatsby serve",
		"clean": "gatsby clean",
		"test": "jest",
		"test:debug": "jest --runInBand",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage=true",
		"test:update": "jest --verbose --updateSnapshot"
	},
	"author": "Imran Sayed",
	"license": "MIT",
	"dependencies": {
		"dotenv": "^8.2.0",
		"gatsby": "^2.21.33",
		"gatsby-wordpress-theme-phoenix": "*",
		"react": "^16.13.1",
		"react-dom": "^16.13.1"
	},
	"devDependencies": {}
}
